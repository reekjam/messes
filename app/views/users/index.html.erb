  <div class='row'>
    <div class='buttons-nav'>
      <div class='small-12 large-6 columns'>
        <%= link_to "Month", "#", class: "button large", data: {dropdown: 'dropdown-months'} %>
        <ul id="dropdown-months" class="medium f-dropdown" data-dropdown-content>
          <% @months.each do |month| %>
            <li><a href="#"><%= month %></a></li>
          <% end %>
        </ul>
      </div>
      <div class='small-12 large-6 columns'>
        <%= link_to "Year", "#", class: "button large", data: { dropdown: 'dropdown-years' } %>
        <ul id="dropdown-years" class="medium content f-dropdown content" data-dropdown-content>
          <li>(years)</li>
      </div>
    </div>
  </div>
  <div class='row'>
    <%= render partial: 'photo', collection: @photos %>
  </div>
