  <div class='row'>
    <% @years.each do |year| %>
      <div class='small-12 large-12 columns'><span class='year-break'><%= year %></span></div><hr></hr>
      <div class='row'>
        <% @sorted_photos.each do |photo| %>
        <div class='small-4 large-2 columns'>
          <% if get_year(photo.created_time) == year %>
            <div class='user-photo th'><img src='<%= photo.images.thumbnail.url %>'></div>
          <% end %>
        </div>
        <% end %>
      </div>
    <% end %>
  </div>
